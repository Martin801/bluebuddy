<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="9"
        android:background="#ffffff"
        android:orientation="vertical"
        >

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#797878"></View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_2sdp"
            android:layout_marginTop="@dimen/_1sdp"
            android:orientation="horizontal"
            android:weightSum="5">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|center_vertical"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/img_allevt"
                        style="@style/menu_img_footer"
                        android:layout_gravity="center"
                       android:src="@drawable/date" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TRIPS"
                        style="@style/footer_text"
                        android:textStyle="normal" />

                </LinearLayout>

                <Button
                    android:id="@+id/btn_AllEventft"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:background="@android:color/transparent"
                    android:textAllCaps="false"
                    android:textColor="#636363"

                    android:textStyle="normal" />

            </FrameLayout>

            <View
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="#757575" />

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|center_vertical"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/img_evtsrch"
                        style="@style/menu_img_footer"
                        android:layout_gravity="center"
                        android:src="@drawable/zoom" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="SEARCH"
                        style="@style/footer_text"
                        android:textStyle="normal" />

                </LinearLayout>

                <Button
                    android:id="@+id/btn_evtsrch"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:background="@android:color/transparent"
                    android:textAllCaps="false"
                    android:textColor="#636363"
                  android:textStyle="normal" />


            </FrameLayout>

            <View
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="#757575" />

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|center_vertical"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/img_bmrkt"
                        style="@style/menu_img_footer"
                        android:layout_gravity="center"
                        android:src="@drawable/blue_market" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                         android:text="MARKET"
                        style="@style/footer_text"
                        android:textStyle="normal" />


                </LinearLayout>

                <Button
                    android:id="@+id/btn_bmrkt"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:background="@android:color/transparent"
                    android:textAllCaps="false"
                    android:textColor="#636363"
                    android:textSize="15dp"
                    android:textStyle="normal" />

            </FrameLayout>

            <View
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="#757575" />

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|center_vertical"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/img_chat"
                        style="@style/menu_img_footer"
                        android:layout_gravity="center"
                        android:src="@drawable/chat" />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="CHAT"
                        style="@style/footer_text"
                        android:textStyle="normal" />

                </LinearLayout>

                <Button
                    android:id="@+id/btn_chat"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@android:color/transparent"
                    android:gravity="center"
                    android:textAllCaps="false"
                    android:textColor="#636363"

                    android:textStyle="normal" />

            </FrameLayout>

            <View
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="#757575" />

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|center_vertical"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/img_accnt"
                        style="@style/menu_img_footer"
                        android:layout_gravity="center"

                        android:src="@drawable/user" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="ACCOUNT"
                        style="@style/footer_text"
                        android:textStyle="normal" />

                </LinearLayout>

                <Button
                    android:id="@+id/btn_Account"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:background="@android:color/transparent"
                    android:textAllCaps="false"
                    android:textColor="#636363"
                    android:textStyle="normal" />

            </FrameLayout>

        </LinearLayout>
    </LinearLayout>

</merge>
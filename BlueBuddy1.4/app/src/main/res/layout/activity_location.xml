<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@drawable/cmnbg"
    android:layout_height="match_parent"
    tools:context="com.bluebuddy.activities.CreateProfileActivity6"
    android:gravity="left"
    android:weightSum="100">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="7">
        <include layout="@layout/navlayout1" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_weight="93"
        android:orientation="vertical"
        android:weightSum="100">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="15"
            android:gravity="center_horizontal|center_vertical|left"
            android:orientation="vertical"
            android:paddingTop="24dp">

            <TextView
                android:id="@+id/idtxt1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:text="@string/txt_60"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="25sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="20"
            android:gravity="left"
            android:orientation="vertical">

           <!-- <EditText
                android:id="@+id/weblnkid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint="@string/txt_61"
                android:inputType="textUri"
                android:lines="1"
                android:textSize="16sp" />-->

            <AutoCompleteTextView
                android:id="@+id/autoCompleteTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:background="@android:color/transparent"
                android:hint="Please enter your place" >

            </AutoCompleteTextView>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="10dp"
                android:background="#757575" />

      <!--     <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
               android:orientation="vertical">

                <AutoCompleteTextView
                    android:id="@+id/autoCompleteTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:background="@android:color/transparent"
                    android:hint="Please enter your place" >
                    <requestFocus />
                </AutoCompleteTextView>

               <View
                   android:layout_width="match_parent"
                   android:layout_height="1px"
                   android:layout_alignParentBottom="true"
                   android:layout_marginTop="10dp"
                   android:background="#757575" />

            </LinearLayout>-->

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|center_vertical"
            android:layout_marginTop="35dp"
            android:layout_weight="35"
            android:orientation="horizontal">

            <Button
                android:id="@+id/weblnknxtid"
                android:layout_width="0dp"
                android:layout_height="45dp"
                android:layout_marginRight="15dp"
                android:layout_weight="1"
                android:background="@drawable/round_corner1"
                android:text="@string/btn_cpnxt"
                android:textColor="#FFFFFF"
                android:textSize="16sp" />

        </LinearLayout>

    </LinearLayout>


</LinearLayout>